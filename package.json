{
  "name": "rex",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "dev": "webpack --config config/webpack.config.js --watch --quiet --no-info",
    "start": "NODE_ENV=development nodemon --watch server ./server/ES6_entry.js",
    "build:assets": "NODE_ENV=production babel public --out-dir dist/public --copy-files",
    "build:assets:staging": "NODE_ENV=staging babel public --out-dir dist/public --copy-files",
    "build:server": "NODE_ENV=production babel server --out-dir dist/server --copy-files",
    "build:server:staging": "NODE_ENV=staging babel server --out-dir dist/server --copy-files",
    "build:src": "NODE_ENV=production webpack --config config/webpack.config.js",
    "build:src:staging": "NODE_ENV=staging webpack --config config/webpack.config.js",
    "build:staging": "rm -rf dist && npm run build:src:staging && npm run build:server:staging && npm run build:assets:staging",
    "build:production": "rm -rf dist && npm run build:src && npm run build:server && npm run build:assets",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run flow status && yarn run test"
    }
  },
  "dependencies": {
    "body-parser": "~1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "~1.4.3",
    "debug": "~3.1.0",
    "dotenv": "^6.0.0",
    "ejs": "^2.5.5",
    "express": "~4.16.3",
    "jsonwebtoken": "^8.3.0",
    "morgan": "~1.9.0",
    "serve-favicon": "~2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0-0",
    "@babel/polyfill": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0",
    "connected-react-router": "^4.4.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "date-fns": "^1.29.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.83.0",
    "history": "^4.7.2",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "preact": "^8.2.9",
    "preact-compat": "^3.18.0",
    "prop-types": "^15.6.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-redux": "^4.0.8",
    "react-test-renderer": "^16.5.2",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.1",
    "redux-thunk": "^2.3.0",
    "resolve-url-loader": "^2.3.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "styled-components": "^3.4.5",
    "webpack": "^4.17.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0"
  }
}
